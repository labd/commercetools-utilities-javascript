{
  "version": "0.7.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "pnpm": "8.1.1",
    "node": ">=16"
  },
  "packageManager": "pnpm@8.1.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsup src --watch",
    "build": "tsup src",
    "publish:ci": "pnpm build && pnpm changeset publish",
    "test": "jest test --coverage",
    "lint": "eslint src/ --ext ts",
    "prepare": "pnpm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@labdigital/commercetools-utilities",
  "author": "Lab Digital",
  "homepage": "https://github.com/labd/commercetools-utilities-javascript",
  "repository": "github:labd/commercetools-utilities-javascript",
  "module": "dist/commercetools-utilities.esm.js",
  "size-limit": [
    {
      "path": "dist/commercetools-utilities.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/commercetools-utilities.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-typescript": "7.14.5",
    "@changesets/cli": "^2.26.0",
    "@changesets/changelog-github": "^0.4.8",
    "@commercetools/platform-sdk": "2.7.0",
    "@labdigital/commercetools-mock": "1.0.0",
    "@labdigital/eslint-config-node": "0.0.5",
    "@size-limit/preset-small-lib": "4.9.1",
    "@types/node": "16.4.0",
    "@types/node-fetch": "2.5.11",
    "aws-sdk": "2.795.0",
    "husky": "4.3.6",
    "jest": "^28.1.3",
    "nock": "13.1.1",
    "size-limit": "4.9.1",
    "tsdx": "0.14.1",
    "tslib": "2.0.3",
    "tsup": "^6.7.0",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@commercetools/platform-sdk": "^2.7.0",
    "node-fetch": "^2.6.1"
  },
  "dependencies": {
    "@commercetools/sdk-client-v2": "^1.4.0"
  }
}
