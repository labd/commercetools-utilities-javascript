{
  "version": "0.7.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@labdigital/commercetools-utilities",
  "author": "Lab Digital",
  "homepage": "https://github.com/labd/commercetools-utilities-javascript",
  "repository": "github:labd/commercetools-utilities-javascript",
  "module": "dist/commercetools-utilities.esm.js",
  "size-limit": [
    {
      "path": "dist/commercetools-utilities.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/commercetools-utilities.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-typescript": "7.14.5",
    "@commercetools/platform-sdk": "2.7.0",
    "@labdigital/commercetools-mock": "0.5.23",
    "@size-limit/preset-small-lib": "4.9.1",
    "@types/node": "16.4.0",
    "@types/node-fetch": "2.5.11",
    "aws-sdk": "2.795.0",
    "husky": "4.3.6",
    "nock": "13.1.1",
    "size-limit": "4.9.1",
    "tsdx": "0.14.1",
    "tslib": "2.0.3",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@commercetools/platform-sdk": "^2.7.0",
    "node-fetch": "^2.6.1"
  }
}
